{
  "name": "@xray/ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Shared UI components for Xray",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "imports": {
    "#*": [
      "./*",
      "./*.ts",
      "./*.tsx"
    ],
    "#shared/*": "../../shared/*",
    "#storybook/*": "./.storybook/*"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@xray/schema": "workspace:*",
    "solid-js": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-solid": "^2.10.2",
    "storybook": "^10.1.9",
    "storybook-solidjs-vite": "^10.0.9",
    "playwright": "^1.57.0",
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.1.9",
    "@storybook/addon-docs": "^10.1.9",
    "@storybook/addon-links": "^10.1.9",
    "@storybook/addon-onboarding": "^10.1.9",
    "@storybook/addon-vitest": "^10.1.9",
    "@storybook/builder-vite": "^10.1.9",
    "@vitest/browser": "^4.0.15",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15"
  }
}
